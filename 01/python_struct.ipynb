    "import struct\n",
    "\n",
    "# pack 和 unpack\n",
    "# '>' 代表big-endian编码\n",
    "# 'I' 代表4个字节的整数\n",
    "# 'H' 代表2字节的无符号整数\n",
    "print(struct.pack('> I', 1050624))\n",
    "print(struct.pack('> I', 10240064))\n",
    "print(struct.unpack('> IH', b'\\x00\\x9c\\x40\\x40\\x7c\\x80'))\n"
   ]
  }
